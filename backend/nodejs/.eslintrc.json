{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["warn", 4],
        "linebreak-style":"off",
        "no-undef":"off",
        "no-multi-spaces":"off",
        "comma-dangle":"off",
        "max-len": ["warn", 120]
    }
}
